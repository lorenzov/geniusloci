html,body {
	margin:0;
	padding:0;
	height:100%;
	text-align: center; /* IE 5.5 Hack */
	font-family:"Gill Sans","Verdana";
	font-size: 84%;
	color: #3e3e3e;
	background: white;
	overflow-x:hidden;
	/*background: white url(http://static.toolmeet.com/images/bg.jpg) top left repeat-x;*/
}

a {
	color: #818285;
	text-decoration: none;
	outline: none;
}

a.img {
	border: none;
	text-decoration: none;
	display: block;
	float: left;
	background-color: transparent;
}

a:hover {
	text-decoration: none;
	color: #3e3e3e;
	border: 0px;
}

a.image,
a.image:hover,
a.image img, a img {
	border: none !important;
	text-decoration: none;
	background-color: transparent;
}

blockquote {
	padding: 10px 10px;
	color: #333;
}

fieldset {
	border:none;
	padding:0;
	margin:0;
}

h1{
	font-size: 26px;
	font-weight:normal;
}

h2{
	font-size: 15px;
	font-weight:normal;
}

h3{
	font-size: 20px;
	font-weight:normal;
}

h4{
	font-size: 20px;
	font-weight:normal;
}

h1 a, h2 a, h3 a {
	border:0;
}

h1 a:hover, h2 a:hover, h3 a:hover {
	color: #3e3e3e;
	background-color: transparent;
}

p {
	line-height: 1em;
	padding: .25em 0 0 0;
	margin: 0;
}

ul li{
	line-height: 2.0em;
	padding: 0;
	margin: 0 0 2px 20px;
} 

ul {
	padding: 2px 0 0 0;
	margin: 10px 0 10px 5px;
}

label {
	font-size: 110%;
	line-height: 1.75em;
	color: #000;
	padding: 0;
	margin: 0;
}

input{
	outline:0;
}

/*-------------------------------Container ----------------------------------*/
div.container {
	width: 750px;
	height:100%;
	margin: 0 auto;
	padding: 0;
	text-align: left;
	background-color: transparent;
	position:relative;
}

div#container {
	position:relative; /* needed for footer positioning*/
	margin:0 auto; /* center, not in IE5 */
	width:100%;
	
	height:auto !important; /* real browsers */
	height:100%; /* IE6: treaded as min-height*/

	min-height:100%; /* real browsers */
}

/*------------------------------- Header ------------------------------------*/
div#header {
	margin: 0;
	padding: 0;
	height: 116px;
	position:relative;	
}

div#header #logo {
	width: 150px;
	text-align:left;
	display: block;
	border-style: none;
	position:relative;
	top:20px;
}

/*------------------------------- Footer ----------------------------------*/

div#footer {
	background-color: #ededed;
	position:absolute;
	width:100%;
	bottom:0; /* stick to bottom */
	padding: 0;
	margin:  0;
	text-align:left;
	height:  41px;
	font-size:  100%;
	left:0;
	border-top:1px solid #818285;
}

div#footer  a{
	text-decoration:underline;
}

div#footer-content {
	padding:15px 0 0 0;
}

div#footer-left {
	float:left;
	width:auto;
	font-size:10px;
	font-weight:bold;
}

div#footer-right {
	float:right;
	width:auto;
	font-size:10px;
	font-weight:bold;
}

